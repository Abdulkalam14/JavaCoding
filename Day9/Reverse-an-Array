#####https://www.geeksforgeeks.org/problems/reverse-an-array/1
class Solution {
    public void reverseArray(int arr[]) {
        // code here
        int n=arr.length;
         //int[] res=new int[n];
        for(int i=0;i<n/2;i++)
        {
            int temp=arr[i];
            arr[i]=arr[n-1-i];
            arr[n-1-i]=temp;
            
        }
       
    }
    
}
