https://www.geeksforgeeks.org/batch/java-foundation-2/track/Java-Foundation-String-2/problem/reverse-string--141628

class Solution {
    public static String reverseString(String s) {
        // code here
        String res="";
        int n=s.length();
        for(int i=n-1;i>=0;i--)
        {
            res+=s.charAt(i);
        }
        return res;
    }
}
