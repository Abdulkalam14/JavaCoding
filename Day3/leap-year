import java.util.*;

public class Solution {
    public static void solve() {
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
        // code here
        if(n%4==0&&n%100!=0)
        {
            System.out.println("True");
        }
        else if(n%400==0)
        {
            System.out.println("True");
        }
        else{
            System.out.println("False");
        }
    }
}
